<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <include domain="root" />
        <include domain="file" />
        <include domain="database" />
        <include domain="sharedpref" />

        <exclude domain="external" />
        <exclude domain="sharedpref" path="device.xml"/>

    </cloud-backup>
    <device-transfer>
        <include domain="root" />
        <include domain="file" />
        <include domain="database" />
        <include domain="sharedpref" />

        <exclude domain="external" />
        <exclude domain="sharedpref" path="device.xml"/>
    </device-transfer>
</data-extraction-rules>
